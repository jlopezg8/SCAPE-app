{
  "expo": {
    "name": "SCAPE-app",
    "slug": "SCAPE-app",
    "version": "1.0.0",
    "icon": "./assets/images/icon.png",
    "scheme": "scape-app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "description": "App para el control de asistencia del personal mediante biometría (reconocimiento facial) y georreferenciación.\n\nPresentado como proyecto de la asignatura de Ingeniería de Software III 2021-1 del programa de Ingeniería en Sistemas y Computación de la Universidad de Caldas.",
    "githubUrl": "https://github.com/jlopezg8/SCAPE-app"
  }
}
